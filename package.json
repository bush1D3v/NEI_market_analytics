		"tailwind-merge": "^2.5.2",
		"tailwindcss-animate": "^1.0.7",
		"vue": "^3.4.29",
		"vue-router": "^4.3.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
